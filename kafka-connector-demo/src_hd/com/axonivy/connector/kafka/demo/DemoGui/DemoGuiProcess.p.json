      "name" : "Send PersonTopic",
      "config" : {
        "output" : {
          "code" : [
            "import java.util.concurrent.Future;",
            "import org.apache.kafka.clients.producer.ProducerRecord;",
            "import org.apache.kafka.clients.producer.Producer;",
            "import com.axonivy.connector.kafka.KafkaService;",
            "import org.apache.kafka.clients.producer.RecordMetadata;",
            "import com.axonivy.connector.kafka.demo.DemoService;",
            "import com.axonivy.connector.kafka.demo.Person;",
            "",
            "Person person = DemoService.get().createRandomPerson();",
            "",
            "// Send from Java, so that the Person.class will be available. Wait for return.",
            "RecordMetadata md = KafkaService.get().send(\"localhostSchematic\", \"PersonTopic\", person.id, person, null).get() as RecordMetadata;",
            "",
            "ivy.log.info(\"Metadata: offset: {0}\", md.offset());"
          ]
        }
